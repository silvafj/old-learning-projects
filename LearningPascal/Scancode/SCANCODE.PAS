    Write(5,5,7,'Caps              : ');
    Write(5,6,7,'NumLock           : ');
    Write(5,7,7,'Scroll            : ');
    Write(5,8,7,'Insert            : ');
    Write(5,9,7,'Left Alt          : ');
    Write(5,10,7,'Right Alt         : ');
