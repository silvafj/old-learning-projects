      Write(26,5,10,IsTrue[CapsIsOn]);
      Write(26,6,10,IsTrue[NumLockIsOn]);
      Write(26,7,10,IsTrue[ScrollIsOn]);
      Write(26,8,10,IsTrue[InsertIsOn]);
      Write(26,9,10,IsTrue[LAltIsPressed]);
      Write(26,10,10,IsTrue[RAltIsPressed]);
      Write(26,11,10,IsTrue[LCtrlIsPressed]);
      Write(26,12,10,IsTrue[RCtrlIsPressed]);
      Write(26,13,10,IsTrue[LeftShiftIsPressed]);
      Write(26,14,10,IsTrue[RightShiftIsPressed]);
