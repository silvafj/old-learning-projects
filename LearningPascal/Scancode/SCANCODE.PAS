    Horizontal(18,17,24,9,0,'Ä');
    Write(32,16,9,'Key Type');
    Horizontal(32,17,39,9,0,'Ä');
    Write(45,16,9,'Character');
    Horizontal(45,17,53,9,0,'Ä');
    Write(59,16,9,'Final Defined Key');
    Horizontal(59,17,75,9,0,'Ä');
