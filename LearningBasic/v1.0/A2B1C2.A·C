    ON ERROR GOTO 10
    CLS
    INPUT "Arquivo a excluir: "; f$
    KILL f$
    GOTO 11
10  LOCATE 10, 15: PRINT "ARQUIVO NÇO ENCONTRADO"
    SLEEP (5)
    CHAIN "A1B1C1.AúC"
11  LOCATE 10, 15: PRINT "OPERA€ÇO CONCLUÖDA"
    SLEEP (2)
    LOCATE 11, 13: PRINT "VOLTE SEMPRE... EU EXCLUI-O TUDO QUE QUISER..."
    SLEEP (3)
    CHAIN "A1B1C1.AúC"

